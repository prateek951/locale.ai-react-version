  "dependencies": {
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "csv-parser": "^2.2.0",
    "express": "^4.16.4",
    "fast-csv": "^2.4.1",
    "http-status-codes": "^1.3.2",
    "multer": "^1.4.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "nodemon": "^1.18.10"
  }
}
